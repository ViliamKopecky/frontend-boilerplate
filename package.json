{
	"name": "react-webpack-typescript-starter",
	"version": "0.1.0",
	"description": "Starter kit for React, Webpack (with Hot Module Replacement), Typescript and Babel.",
	"keywords": [
		"react",
		"webpack",
		"typescript",
		"babel",
		"sass",
		"hmr",
		"starter",
		"boilerplate"
	],
	"author": "Viktor Persson",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/vikpe/react-webpack-typescript-starter.git"
	},
	"bugs": {
		"url": "https://github.com/vikpe/react-webpack-typescript-starter/issues"
	},
	"homepage": "https://github.com/vikpe/react-webpack-typescript-starter",
	"scripts": {
		"build": "npm run clean-dist && webpack -p --config=configs/webpack/prod.js",
		"clean-dist": "rm -f -r -d dist",
		"lint": "npm run lint:ts && npm run lint:sass",
		"lint:ts": "tslint \"./src/**/*.ts*\" -e \"./src/**/*.module.sass.d.ts*\" --format stylish --force",
		"lint:ts:fix": "tslint \"./src/**/*.ts*\" -e \"./src/**/*.module.sass.d.ts*\" --format stylish --force --fix",
		"lint:sass": "stylelint \"./src/**/*.s{a,c}ss\"",
		"start": "npm run start-dev",
		"start-dev": "webpack-dev-server --config=configs/webpack/dev.js",
		"start-prod": "npm run build && node express.js",
		"test": "jest --watch --coverage --config=configs/jest.json",
		"prettier": "prettier --write \"{./,./src/**/}*.{ts,tsx,js,jsx}\""
	},
	"devDependencies": {
		"@types/jest": "^22.2.3",
		"@types/node": "^10.1.4",
		"@types/react": "^16.3.14",
		"@types/react-dom": "^16.0.5",
		"awesome-typescript-loader": "^5.0.0",
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.3",
		"babel-loader": "^7.1.4",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.24.1",
		"css-loader": "^0.28.11",
		"express": "^4.16.3",
		"file-loader": "^1.1.11",
		"html-webpack-plugin": "^3.2.0",
		"image-webpack-loader": "^4.2.0",
		"jest": "^22.4.4",
		"node-sass": "^4.9.0",
		"postcss-loader": "^2.1.5",
		"pug": "^2.0.3",
		"pug-loader": "^2.4.0",
		"react": "^16.4.0",
		"react-addons-test-utils": "^15.6.2",
		"react-dom": "^16.4.0",
		"react-hot-loader": "^4.2.0",
		"sass-loader": "^7.0.1",
		"style-loader": "^0.21.0",
		"stylelint": "^9.2.1",
		"stylelint-config-recommended": "^2.1.0",
		"stylelint-webpack-plugin": "^0.10.5",
		"tslint": "^5.10.0",
		"typescript": "^2.8.4",
		"typings-for-css-modules-loader": "^1.7.0",
		"uglifyjs-webpack-plugin": "^1.2.5",
		"webpack": "^4.10.2",
		"webpack-cli": "^2.1.4",
		"webpack-dev-middleware": "^3.1.3",
		"webpack-dev-server": "^3.1.4",
		"webpack-manifest-plugin": "^2.0.3",
		"webpack-merge": "^4.1.2"
	},
	"postcss": {}
}
